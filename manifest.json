{
  "name": "Remove YouTube Shorts",
  "manifest_version": 3,
  "version": "0.1",
  "description": "Remove Shorts from YouTube",
  "permissions": ["scripting"],
  "icons": { "128": "128.png" },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content_scripts.js"]
    }
  ]
}
